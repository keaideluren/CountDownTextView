<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CountDownTextView" >
        <attr name="textBefore" format="string" />
        <attr name="textAfter" format="string" />
        <attr name="maxTime" format="enum">
            <enum name="year" value="5" />
            <enum name="month" value="4" />
            <enum name="day" value="3" />
            <enum name="hour" value="2" />
            <enum name="minute" value="4" />
            <enum name="seconds" value="0" />
        </attr>
        <attr name="isFillLength" format="boolean" />
        <attr name="isShowHighOrderZero" format="boolean" />
        <attr name="beforeTextColor" format="color" />
        <attr name="afterTextColor" format="color" />
        <attr name="beforeTextSize" format="dimension" />
        <attr name="afterTextSize" format="dimension" />
    </declare-styleable>
</resources>